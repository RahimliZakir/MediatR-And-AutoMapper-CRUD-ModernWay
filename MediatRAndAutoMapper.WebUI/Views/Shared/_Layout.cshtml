<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/libs/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/libs/limonte-sweetalert2/sweetalert2.min.css" />
    @await RenderSectionAsync("css", false);
</head>
<body>
    <div class="root">
        <div class="container">
            <div class="row vh-100 justify-content-center">
                    @RenderBody()
            </div>
        </div>
    </div>

    <script src="~/libs/jquery/jquery.min.js"></script>
    <script src="~/libs/limonte-sweetalert2/sweetalert2.all.min.js"></script>
     @await RenderSectionAsync("js", false);
</body> 
</html>
